#include <dt-bindings/zmk/keys.h>
#include <behaviors.dtsi>
#include <dt-bindings/zmk/kscan_mock.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &nkp A &kp W
                &nkp D &mo 1>;
        };

        lower_layer {
            bindings = <
                &kp LEFT_SHIFT &kp LEFT_CONTROL
                &kp Y  &kp Z>;
        };
    };
};
